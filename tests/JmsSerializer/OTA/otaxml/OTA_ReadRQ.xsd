<?xml version="1.0" encoding="UTF-8"?><xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.opentravel.org/OTA/2003/05" targetNamespace="http://www.opentravel.org/OTA/2003/05" elementFormDefault="qualified" version="1.009" id="OTA2007B">	<xs:include schemaLocation="OTA_CruiseCommonTypes.xsd"/>	<xs:include schemaLocation="OTA_PkgCommonTypes.xsd"/>	<xs:include schemaLocation="OTA_VehRetResRQ.xsd"/>	<xs:annotation>		<xs:documentation xml:lang="en">All Schema files in the OTA specification are made available according to the terms defined by the OTA License Agreement at http://www.opentravel.org/ota_downloads_form.cfm</xs:documentation>	</xs:annotation>	<xs:element name="OTA_ReadRQ">		<xs:annotation>			<xs:documentation xml:lang="en">
A generic message, available as an action on several OTA services which requests a server to read and return the document type identified by the UniqueID element.
			</xs:documentation>		</xs:annotation>		<xs:complexType>			<xs:sequence>				<xs:element name="POS" type="POS_Type" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="en">Identifies the party or connection channel making the request.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="UniqueID" type="UniqueID_Type" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="en">A reference to identify the booking.</xs:documentation>					</xs:annotation>				</xs:element>				<xs:element name="ReadRequests" minOccurs="0">					<xs:annotation>						<xs:documentation xml:lang="en">A grouping of Read Requests</xs:documentation>					</xs:annotation>					<xs:complexType>						<xs:choice>							<xs:element name="ReadRequest" minOccurs="0" maxOccurs="unbounded">								<xs:annotation>									<xs:documentation xml:lang="en">A request to read a reservation when the booking reference is known. </xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="UniqueID" type="UniqueID_Type">											<xs:annotation>												<xs:documentation xml:lang="en">The booking reference for the reservation.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="Verification" type="VerificationType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">This is to be used for verification that the record being sent is the specific record requested. This is optional and is intended to provide a more secure mechanism to ensure the correct record is being sent. </xs:documentation>											</xs:annotation>										</xs:element>									</xs:sequence>									<xs:attribute name="HistoryRequestedInd" type="xs:boolean" use="optional">										<xs:annotation>											<xs:documentation xml:lang="en">When true, the history of the reservation should be returned.</xs:documentation>										</xs:annotation>									</xs:attribute>								</xs:complexType>							</xs:element>							<xs:element name="GlobalReservationReadRequest" minOccurs="0" maxOccurs="unbounded">								<xs:annotation>									<xs:documentation xml:lang="en">An individual Read Request for a global reservation when the booking reference is not known</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="TravelerName" type="PersonNameType">											<xs:annotation>												<xs:documentation xml:lang="en">The name of the traveler for whom the reservation is requested.</xs:documentation>											</xs:annotation>										</xs:element>									</xs:sequence>									<xs:attributeGroup ref="DateTimeSpanGroup">										<xs:annotation>											<xs:documentation xml:lang="en">The travel start date.</xs:documentation>										</xs:annotation>									</xs:attributeGroup>								</xs:complexType>							</xs:element>							<xs:element name="HotelReadRequest" minOccurs="0" maxOccurs="unbounded">								<xs:annotation>									<xs:documentation xml:lang="en">To retrieve a hotel reservation when the booking reference is not known. </xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="CityName" type="StringLength1to64" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">City name eg. Dublin</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="Airport" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">Airport code and airport name for the location of the hotel.</xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:attribute name="LocationCode" type="StringLength1to8" use="optional">													<xs:annotation>														<xs:documentation xml:lang="en">The airport code for the location of the hotel.</xs:documentation>													</xs:annotation>												</xs:attribute>												<xs:attribute name="CodeContext" type="StringLength1to32" default="IATA">													<xs:annotation>														<xs:documentation xml:lang="en">Identifies the context of the location code, such as IATA, ARC, or internal code, etc.</xs:documentation>													</xs:annotation>												</xs:attribute>												<xs:attribute name="AirportName" type="StringLength1to64" use="optional">													<xs:annotation>														<xs:documentation xml:lang="en">The airport location name.</xs:documentation>													</xs:annotation>												</xs:attribute>											</xs:complexType>										</xs:element>										<xs:element name="UserID" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">The user id and pin number of the person for whom the reservation is for.</xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:complexContent>													<xs:extension base="UniqueID_Type">														<xs:attribute name="PinNumber" type="StringLength1to16" use="optional"/>													</xs:extension>												</xs:complexContent>											</xs:complexType>										</xs:element>										<xs:element name="Verification" type="VerificationType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">This is to be used for verification that the record being sent is the specific record requested.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element ref="TPA_Extensions" minOccurs="0"/>									</xs:sequence>									<xs:attributeGroup ref="HotelReferenceGroup">										<xs:annotation>											<xs:documentation xml:lang="en">The hotel name, hotel code, and city code for the hotel booked in a reservation.</xs:documentation>										</xs:annotation>									</xs:attributeGroup>								</xs:complexType>							</xs:element>							<xs:element name="AirReadRequest" minOccurs="0" maxOccurs="unbounded">								<xs:annotation>									<xs:documentation xml:lang="en">To retrieve an airline reservation when the booking reference is not known or to search for a list of passengers meeting certain criteria </xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="POS" type="POS_Type" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">To request a list of reservations made by specified travel agent, ERSP, booking channel, etc.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="Airline" type="CompanyNameType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">Specify airline on which the person is booked.									</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="FlightNumber" type="FlightNumberType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">Specific flight number on which the passenger is booked.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="DepartureAirport" type="LocationType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">Flight Origin - IATA 3 letter code.  </xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="DepartureDate" type="xs:date" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">Request a reservation with this departure date.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="Name" type="PersonNameType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">Request a reservation based on a passenger name.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="Telephone" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">Request a reservation based on a telephone number.</xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:attributeGroup ref="TelephoneGroup"/>											</xs:complexType>										</xs:element>										<xs:element name="CustLoyalty" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">Request booking records based on frequent flyer information.</xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:attributeGroup ref="CustomerLoyaltyGroup"/>											</xs:complexType>										</xs:element>										<xs:element name="CreditCardInfo" type="PaymentCardType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">Request booking records by credit card information.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="TicketNumber" type="TicketingInfoRS_Type" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">Request booking record by ticket number.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="QueueInfo" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">Requests items from a booking file queue. </xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:sequence>													<xs:element name="Queue" maxOccurs="99">														<xs:annotation>															<xs:documentation xml:lang="en">Specifies queue information.</xs:documentation>														</xs:annotation>														<xs:complexType>															<xs:attributeGroup ref="QueueGroup"/>														</xs:complexType>													</xs:element>												</xs:sequence>												<xs:attribute name="FirstItemOnlyInd" type="xs:boolean">													<xs:annotation>														<xs:documentation xml:lang="en">If true, only the first item on the queue is to be returned. If false, all items on the queue are to be returned.</xs:documentation>													</xs:annotation>												</xs:attribute>												<xs:attribute name="RemoveFromQueueInd" type="xs:boolean">													<xs:annotation>														<xs:documentation xml:lang="en">If true, items are to be removed from the queue after display. If false, items are to remain on the queue after display. If this attribute is omitted, items on the queue are to be retained on the queue.</xs:documentation>													</xs:annotation>												</xs:attribute>												<xs:attribute name="FullDataInd" type="xs:boolean">													<xs:annotation>														<xs:documentation xml:lang="en">Option to define if the full booking file data need to be returned. If true, all booking file info is returned. If false, only the condensed info is returned.</xs:documentation>													</xs:annotation>												</xs:attribute>												<xs:attribute name="StartDate" type="DateOrDateTimeType">													<xs:annotation>														<xs:documentation xml:lang="en">Bookings modified after this start date or date time should be returned. If used in conjunction with an end date, only bookings modified within that date range should be returned.</xs:documentation>													</xs:annotation>												</xs:attribute>												<xs:attribute name="EndDate" type="DateOrDateTimeType">													<xs:annotation>														<xs:documentation xml:lang="en">Bookings modified before this end date or date time should be returned. If used in conjunction with a start date, only bookings modified within that date range should be returned.</xs:documentation>													</xs:annotation>												</xs:attribute>											</xs:complexType>										</xs:element>										<xs:element ref="TPA_Extensions" minOccurs="0"/>									</xs:sequence>									<xs:attribute name="SeatNumber" type="xs:string" use="optional">										<xs:annotation>											<xs:documentation xml:lang="en">To specify a seat number to be used as criteria for requesting a booking record. </xs:documentation>										</xs:annotation>									</xs:attribute>									<xs:attribute name="IncludeFF_EquivPartnerLev" type="xs:boolean" use="optional">										<xs:annotation>											<xs:documentation xml:lang="en">When requesting booking records by frequent flyer level, this is used to indicate that booking records with the codeshare equivalent partner level should also be returned.</xs:documentation>										</xs:annotation>									</xs:attribute>									<xs:attribute name="ReturnFF_Number" type="xs:boolean" use="optional">										<xs:annotation>											<xs:documentation xml:lang="en">To specify that FF number should be returned for each booking record in the list.  The default is do not return, set to true, the FF number should be returned.</xs:documentation>										</xs:annotation>									</xs:attribute>									<xs:attribute name="ReturnDownlineSeg" type="xs:boolean" use="optional">										<xs:annotation>											<xs:documentation xml:lang="en">To specify that downline segment info should be returned.  The default is do not return, set to true, the information should be returned.</xs:documentation>										</xs:annotation>									</xs:attribute>									<xs:attribute name="InfoToReturn" use="optional">										<xs:annotation>											<xs:documentation xml:lang="en">To specify what type of FF level information should be returned.</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="xs:NMTOKEN">												<xs:enumeration value="ListofFF_StatusLevels"/>												<xs:enumeration value="NoFF_Status"/>												<xs:enumeration value="All"/>											</xs:restriction>										</xs:simpleType>									</xs:attribute>									<xs:attribute name="FF_RequestCriteria" use="optional">										<xs:annotation>											<xs:documentation xml:lang="en">To specify criteria as to whether to return only the FF level specified or levels above and below.</xs:documentation>										</xs:annotation>										<xs:simpleType>											<xs:restriction base="xs:NMTOKEN">												<xs:enumeration value="ReturnLevelAndAbove"/>												<xs:enumeration value="ReturnLevelAndBelow"/>												<xs:enumeration value="ReturnOnlySpecifiedLevel"/>											</xs:restriction>										</xs:simpleType>									</xs:attribute>								</xs:complexType>							</xs:element>							<xs:element name="PkgReadRequest" minOccurs="0" maxOccurs="unbounded">								<xs:annotation>									<xs:documentation xml:lang="en">To retrieve a package reservation record when the booking reference is not known</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="Name" type="PersonNameType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">The name of the lead passenger on the booking.</xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="ArrivalLocation" type="LocationType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">The code for the final destination - normally an IATA airport code. </xs:documentation>											</xs:annotation>										</xs:element>										<xs:element name="DepartureLocation" type="LocationType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">The code for the traveller's departure point - normally an IATA airport code. </xs:documentation>											</xs:annotation>										</xs:element>									</xs:sequence>									<xs:attribute name="TravelCode" type="PkgTravelCode" use="optional">										<xs:annotation>											<xs:documentation xml:lang="en">The supplier's code to identify the travel arrangement for the package</xs:documentation>										</xs:annotation>									</xs:attribute>									<xs:attribute name="TourCode" type="StringLength1to8" use="optional">										<xs:annotation>											<xs:documentation xml:lang="en">The supplier's code to identify the accommodation/tour arrangement for a package</xs:documentation>										</xs:annotation>									</xs:attribute>									<xs:attribute name="PackageID" type="PackageID_RefType" use="optional">										<xs:annotation>											<xs:documentation xml:lang="en">A code identifying a complete package arrangement, replacing individual travel and accommodation codes.</xs:documentation>										</xs:annotation>									</xs:attribute>									<xs:attributeGroup ref="DateTimeSpanGroup">										<xs:annotation>											<xs:documentation xml:lang="en">The start date and duration of the package.</xs:documentation>										</xs:annotation>									</xs:attributeGroup>								</xs:complexType>							</xs:element>							<xs:element name="GolfReadRequest" minOccurs="0" maxOccurs="unbounded">								<xs:annotation>									<xs:documentation xml:lang="en">To retrieve a golf reservation record when the booking reference is not known</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="Membership" minOccurs="0" maxOccurs="99">											<xs:annotation>												<xs:documentation xml:lang="en">To request a reservation record based on golf course membership(s). </xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:attributeGroup ref="CustomerLoyaltyGroup"/>											</xs:complexType>										</xs:element>										<xs:element name="Name" type="PersonNameType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">To request reservation record by the golfer's name. </xs:documentation>											</xs:annotation>										</xs:element>									</xs:sequence>									<xs:attributeGroup ref="ID_Group">										<xs:annotation>											<xs:documentation xml:lang="en">The Golf Course ID.</xs:documentation>										</xs:annotation>									</xs:attributeGroup>									<xs:attribute name="RoundID" type="xs:positiveInteger" use="optional">										<xs:annotation>											<xs:documentation xml:lang="en">A unique identifier applied to this round.</xs:documentation>										</xs:annotation>									</xs:attribute>									<xs:attribute name="PlayDateTime" type="DateOrDateTimeType" use="optional">										<xs:annotation>											<xs:documentation xml:lang="en">The date for which the round is reserved.</xs:documentation>										</xs:annotation>									</xs:attribute>									<xs:attribute name="PackageID" type="StringLength1to8" use="optional">										<xs:annotation>											<xs:documentation xml:lang="en">An identification of a package of which this round is a part.</xs:documentation>										</xs:annotation>									</xs:attribute>								</xs:complexType>							</xs:element>							<xs:element name="VehicleReadRequest" minOccurs="0" maxOccurs="unbounded">								<xs:annotation>									<xs:documentation xml:lang="en">To request a vehicle reservation when the confirmation number is not known.</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:complexContent>										<xs:extension base="VehicleRetrieveResRQCoreType">											<xs:sequence>												<xs:element name="VehRetResRQInfo" type="VehicleRetrieveResRQAdditionalInfoType">													<xs:annotation>														<xs:documentation xml:lang="en">Identifies the core and supplemental information associated with the request for the retrieval of a rental vehicle reservation.</xs:documentation>													</xs:annotation>												</xs:element>											</xs:sequence>										</xs:extension>									</xs:complexContent>								</xs:complexType>							</xs:element>							<xs:element name="CruiseReadRequest" minOccurs="0" maxOccurs="99">								<xs:annotation>									<xs:documentation xml:lang="en">To retrieve cruise booking information when the confirmation number is not known.</xs:documentation>								</xs:annotation>								<xs:complexType>									<xs:sequence>										<xs:element name="SelectedSailing" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">Contains information related to the selected sailing. Typically the selected sailing is one of the sailings returned in the previous sailing availability response. </xs:documentation>											</xs:annotation>											<xs:complexType>												<xs:attributeGroup ref="SelectedSailingGroup"/>												<xs:attribute name="GroupCode" type="StringLength1to16">													<xs:annotation>														<xs:documentation xml:lang="en">Specifies the group code.</xs:documentation>													</xs:annotation>												</xs:attribute>											</xs:complexType>										</xs:element>										<xs:element name="GuestInfo" type="PersonNameType" minOccurs="0">											<xs:annotation>												<xs:documentation xml:lang="en">Contains the guest information such as last name and first name.</xs:documentation>											</xs:annotation>										</xs:element>									</xs:sequence>									<xs:attribute name="HistoryRequestedInd" type="xs:boolean" use="optional">										<xs:annotation>											<xs:documentation xml:lang="en">When true, the history of the reservation should be returned.</xs:documentation>										</xs:annotation>									</xs:attribute>								</xs:complexType>							</xs:element>						</xs:choice>					</xs:complexType>				</xs:element>			</xs:sequence>			<xs:attributeGroup ref="OTA_PayloadStdAttributes"/>			<xs:attributeGroup ref="ReqRespVersion"/>			<xs:attribute name="ReservationType" type="OTA_CodeType">				<xs:annotation>					<xs:documentation xml:lang="en">To specify the type of reservation, Global, Air, Hotel, Car, Package, or Golf, that is being requested.  Refer to OTA Code List Travel Sector (TVS). </xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attribute name="ReturnListIndicator" type="xs:boolean" use="optional">				<xs:annotation>					<xs:documentation xml:lang="en">To indicate if a list should always be returned even when only one reservation matches the input criteria.  The default is to return the reservation record.  If set to true, then always return a list. </xs:documentation>				</xs:annotation>			</xs:attribute>			<xs:attributeGroup ref="ResponseGroup">				<xs:annotation>					<xs:documentation xml:lang="en">Used to indicate a maximum number of items desired in the list response message or when used for subsequent retrievals of additional data the MoreDataEchoToken determines where to continue the list.</xs:documentation>				</xs:annotation>			</xs:attributeGroup>		</xs:complexType>	</xs:element></xs:schema>